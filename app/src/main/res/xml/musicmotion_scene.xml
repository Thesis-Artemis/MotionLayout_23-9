<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>

           <KeyAttribute

               android:scaleX="6"
               android:scaleY="1"
               motion:framePosition="100"
               motion:motionTarget="@+id/imageView10"
               android:rotationX="0" />
           <KeyAttribute

               android:scaleX="0.1"
               android:scaleY="0.1"
               motion:framePosition="100"
               motion:motionTarget="@+id/eclipse"
               android:rotationX="0" />

           <KeyAttribute

               android:scaleX="0.8"
               android:scaleY="1"
               motion:framePosition="100"
               motion:motionTarget="@+id/textView"
               android:rotationX="0" />

           <KeyAttribute

               android:scaleX="0.9"
               android:scaleY="1"
               motion:framePosition="100"
               motion:motionTarget="@+id/imageView5"
               android:rotationX="0" />

       </KeyFrameSet>
        <OnSwipe
            motion:touchAnchorId="@+id/imageView10"
            motion:dragDirection="dragEnd"
            />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView10"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="28dp"
            android:layout_marginStart="28dp"
            android:id="@+id/textView"
            android:layout_marginBottom="60dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView7"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="144dp"
            android:layout_marginStart="144dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView9"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="44dp"
            android:layout_marginRight="44dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView8"
            android:layout_marginEnd="128dp"
            android:layout_marginRight="128dp" />
        <Constraint
            android:layout_height="741dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.545"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="423dp"
            android:id="@+id/imageView10" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="731dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="4dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="412dp"
            android:id="@+id/imageView" />
        <Constraint
            android:layout_height="136dp"
            android:layout_width="415dp"
            android:id="@+id/imageView10"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="0dp" />
        <Constraint
            android:layout_height="78dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="88dp"
            android:id="@+id/imageView3"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.924" />
        <Constraint
            android:layout_height="51dp"
            android:layout_width="373dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:id="@+id/textView"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="44dp" />
        <Constraint
            android:layout_height="496dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="108dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.555"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="373dp"
            android:id="@+id/textView2"
            motion:layout_editor_absoluteY="106dp" />
        <Constraint
            android:layout_height="31dp"
            android:layout_marginBottom="40dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="315dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="92dp"
            android:layout_marginStart="92dp"
            android:id="@+id/imageView5" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="80dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="104dp"
            android:layout_marginStart="104dp"
            android:id="@+id/imageView6" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="80dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/imageView6"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            android:id="@+id/imageView7" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="80dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/imageView7"
            android:layout_marginLeft="36dp"
            android:layout_marginStart="36dp"
            android:id="@+id/imageView8" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="80dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/imageView8"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            android:id="@+id/imageView9" />
    </ConstraintSet>
</MotionScene>